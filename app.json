{
  "expo": {
    "name": "RapidGo Rider",
    "slug": "rapidgo-rider",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rapidgo.rider"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rapidgo.rider",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBJNvG1poWaJjVEpiuMvPpPSo7TpgnZgY4"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "NOTIFICATIONS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": [
    "./plugins/withAndroidNotificationSound.js", 
    "expo-notifications",
    "expo-location",
    [
      "expo-image-picker",
      {
        "photosPermission": "The app accesses your photos to take and upload a profile picture."
      }
    ]
  ],
  
    "extra": {
      "eas": {
        "projectId": "6b3bdd41-3cc2-4890-814b-89fdb2253ca7"
      }
    }
  }
}
